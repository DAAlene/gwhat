@article{kim_spatial_2008,
	title = {Spatial rainfall model using a pattern classifier for estimating missing daily rainfall data},
	volume = {23},
	issn = {1436-3240, 1436-3259},
	url = {http://link.springer.com/article/10.1007/s00477-008-0223-9},
	doi = {10.1007/s00477-008-0223-9},
	abstract = {Missing data in daily rainfall records are very common in water engineering practice. However, they must be replaced by proper estimates to be reliably used in hydrologic models. Presented herein is an effort to develop a new spatial daily rainfall model that is specifically intended to fill in gaps in a daily rainfall dataset. The proposed model is different from a convectional daily rainfall generation scheme in that it takes advantage of concurrent measurements at the nearby sites to increase the accuracy of estimation. The model is based on a two-step approach to handle the occurrence and the amount of daily rainfalls separately. This study tested four neural network classifiers for a rainfall occurrence processor, and two regression techniques for a rainfall amount processor. The test results revealed that a probabilistic neural network approach is preferred for determining the occurrence of daily rainfalls, and a stepwise regression with a log-transformation is recommended for estimating daily rainfall amounts.},
	language = {en},
	number = {3},
	urldate = {2015-02-25},
	journal = {Stochastic Environmental Research and Risk Assessment},
	author = {Kim, Tae-Woong and Ahn, Hosung},
	month = mar,
	year = {2008},
	note = {D267},
	pages = {367--376},
	file = {Kim and Ahn - 2008 - Spatial rainfall model using a pattern classifier .pdf:/home/jsgosselin/.zotero/zotero/zmiix2np.default/zotero/storage/D62MNPTM/Kim and Ahn - 2008 - Spatial rainfall model using a pattern classifier .pdf:application/pdf}
}

