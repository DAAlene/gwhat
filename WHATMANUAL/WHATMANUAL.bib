	file = {Abebe et al. - 2000 - Application of adaptive fuzzy rule-based models fo.pdf:/home/jsgosselin/.zotero/zotero/zmiix2np.default/zotero/storage/FPI52S2D/Abebe et al. - 2000 - Application of adaptive fuzzy rule-based models fo.pdf:application/pdf}
}

