@article{butler_jr._new_2011,
	title = {New {Insights} from {Well} {Responses} to {Fluctuations} in {Barometric} {Pressure}.},
	volume = {49},
	copyright = {Copyright © 2010 The Author(s). Journal compilation © 2010 National Ground Water Association},
	issn = {1745-6584},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1745-6584.2010.00768.x/abstract},
	doi = {10.1111/j.1745-6584.2010.00768.x},
	abstract = {Hydrologists have long recognized that changes in barometric pressure can produce changes in water levels in wells. The barometric response function (BRF) has proven to be an effective means to characterize this relationship; we show here how it can also be utilized to glean valuable insights into semi-confined aquifer systems. The form of the BRF indicates the degree of aquifer confinement, while a comparison of BRFs between wells sheds light on hydrostratigraphic continuity. A new approach for estimating hydraulic properties of aquitards from BRFs has been developed and verified. The BRF is not an invariant characteristic of a well; in unconfined or semi-confined aquifers, it can change with conditions in the vadose zone. Field data from a long-term research site demonstrate the hydrostratigraphic insights that can be gained from monitoring water levels and barometric pressure. Such insights should be of value for a wide range of practical applications.},
	number = {4},
	journal = {Ground Water},
	author = {Butler Jr., J J and Jin, W and Mohammed, G A and Reboulet, E C},
	year = {2011},
	note = {D177},
	keywords = {Barometric Response Function},
	pages = {525--533},
	file = {Butler Jr. et al. - 2011 - New Insights from Well Responses to Fluctuations.pdf:/home/jsgosselin/.zotero/zotero/zmiix2np.default/zotero/storage/FGXFCFCI/Butler Jr. et al. - 2011 - New Insights from Well Responses to Fluctuations.pdf:application/pdf}
}

